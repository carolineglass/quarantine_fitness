<%= form_for @split do |f| %>
    <%= f.label :routine_id %>
    <%= f.collection_select :routine_id, @routines, :id, :name %>

    <%= f.label :exercise_id %>
    <%= f.collection_select :exercise_id, @exercises, :id, :name %>

    <%= f.label :duration "duration in seconds"%>
    <%= f.number_field :duration %>
    
    <%= f.submit "Add Exercise" %>
<% end %>